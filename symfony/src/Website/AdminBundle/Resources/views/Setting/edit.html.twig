{% extends "admin/admin_layout.html.twig" %}
{% block content %}
    
 
 
    <form novalidate="novalidate" action="{{ url('admin_setting_update', { 'id': entity.id }) }}" method="post">

        {% if entity.description %}
            <div class="alert alert-info">
                <button type="button" class="close" data-dismiss="alert">Ã—</button>
                <strong>Information:</strong> {{ entity.description | raw }}
            </div>
        {% endif %}        

    {{ form_rest(form) }}

      <br />
     
        <button class="btn btn-info" type="submit"><i class="icon-save"></i> Save</button>
    
</form>

{% endblock %}

